package com.mime.minefront;

import java.io.File;

public class Configuration {

	public void saveConfiguration(String kry, int value) {
		try {
			File file = new File("res/setting/config.xml");
			boolean exists = file.exists();
			if(!exists) {
				file.createNewFile();
			}
		} catch(Exception e) {
			
		}
	}

}
